{
  "manifest_version": 2,

  "name": "Vitality",
  "description": "Life flows from one moment to the next. Remember your vitality with each new tab.",
  "version": "0.0.3",

  "browser_action": {
    "default_icon": "icon.png"
  },
  "chrome_url_overrides": {
    "newtab": "app.html"
  },
  "permissions": [
    "storage",
    "https://www.google-analytics.com/"
  ]
}
